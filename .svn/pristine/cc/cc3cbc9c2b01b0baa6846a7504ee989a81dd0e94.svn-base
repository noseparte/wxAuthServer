<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true">
	<statusListener class="ch.qos.logback.core.status.OnConsoleStatusListener" />
	<property resource="datasource.properties" />

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger - %msg%n
			</pattern>
		</encoder>
	</appender>
    <!-- 数据库sql_log -->
	<appender name="mybatis_sql_log"
		class="ch.qos.logback.core.rolling.RollingFileAppender">
		<encoder>
			<charset>UTF-8</charset>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger - %msg%n</pattern>
		</encoder>
		<File>/data/logs/h5WxLoginServer/mybatis_sql/mybatis_sql.log</File>
		<rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
			<FileNamePattern>/data/logs/h5WxLoginServer/mybatis_sql/%d{yyyy-MM-dd}.mybatis_sql.log.zip
			</FileNamePattern>
		</rollingPolicy>
		<!-- <layout class="ch.qos.logback.classic.PatternLayout">
			<pattern>%thread<|>%date<|>%-5level<|>%logger<|>%M<|>%msg%n</pattern>
		</layout> -->
	</appender>
	<logger name="mybatis_sql_log" level="INFO">
		<appender-ref ref="mybatis_sql_log" />
	</logger>
	
