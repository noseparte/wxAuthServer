body,body .selectboxit-container * { font-family: Microsoft Yahei;}
.logo-expanded,.logo-collapsed{ color:#fff; font-size:18px;}
.logo-expanded>*{ width:30px; height:30px; vertical-align:middle; margin-right:5px; }
.logo-collapsed img{ width:40px; height:40px; }
.logo-expanded:hover,.logo-collapsed:hover{ color:#fff; font-size:18px;}

.select-app{ text-align:center; }
.select-app .s-logo img{ width:60px; height:60px;}
.select-app .s-logo{ margin-bottom:8px;}
.select-app .form-block label{ cursor:pointer;}

@media (min-width: 768px){#modal-1 .modal-dialog{ width:400px;}}
.upload-area{ margin-top:50px;}
.upload-area .submit-btn{ margin-top:50px;}
.upload-area .table-wrapper{ max-height:475px; overflow:auto;}
.upload-area .table-wrapper table{ margin:0; vertical-align:middle;}

.result-table-wrapper{ margin-top:30px;}
.result-progress-wrapper{ margin-top:10px;}

.user-info-menu .title{ margin:0; line-height:75px;}
.pull-right.recent-meta{ font-size:13px; color: #999;}
.photo-frame img{ width:100%; vertical-align:middle;}
.photo-frame{ margin-bottom:30px;}

.recent{ list-style: none; margin: 0;  padding: 10px;}
.recent li{ border-bottom: 1px dashed #ccc; padding-bottom: 15px; margin-bottom: 15px;}
.recent-meta{ font-size: 15px; color: #333; margin-bottom: 10px;}

.data-range-input{ position: relative;}
.data-range-input em.to{ position: absolute; left: 50%; top:0; height: 32px; line-height: 32px; width: 30px; margin-left: -15px; text-align: center;}

.percent-box { width: 100px; height: 100px; line-height: 100px; color: green; font-size: 22px; text-align: center; border: 2px solid green; border-radius: 100px; display: inline-block; margin-right: 15px;}

.waterfall-wrapper{overflow: hidden; margin-top: 30px;}
.waterfall{ list-style: none; position: relative; padding: 0; margin: 0;}
.waterfall li{
    width: 320px; padding: 10px; background-color: #f2f2f2; border:1px solid #ccc;
    -webkit-transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
    -o-transition: all 0.3s ease-out;
    transition: all 0.3s ease-out;
}
.waterfall .photo{}
.waterfall .photo img{ width: 100%; cursor: pointer;}
.tile-loading:after {
    display: block;
    text-align: center;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: #ddd;
    content: "Loading\2026";
    padding-top: 20px;
    color: #999;
}
.waterfall li .info{ margin-top: 5px;}
.waterfall li .time{ font-size:12px;}
.contrast-btn{ margin-top: 50px; }
.contrast-btn .btn{width: 48%;}
.app-contrast-tabs.tabs-vertical-env > .nav.tabs-vertical{ width: 30px; min-width: inherit;}
.app-contrast-tabs.tabs-vertical-env > .nav.tabs-vertical li > a{ white-space: normal; padding: 10px 5px;}
.app-contrast-tabs.tabs-vertical-env > .nav > li{ width: 30px;}